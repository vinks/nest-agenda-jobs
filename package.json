  },
  "homepage": "https://github.com/vinks/nest-agenda-jobs",
  "peerDependencies": {
    "@nestjs/common": "^5.1.0 || ^6.0.3"
  },
  "dependencies": {
    "agenda": "^2.1.0",
    "bluebird": "^3.7.1"
  },
  "devDependencies": {
    "@nestjs/common": "^6.10.1",
    "@nestjs/core": "^6.10.1",
    "@nestjs/platform-express": "9.2.1",
    "@types/agenda": "^2.0.6",
    "@types/node": "^12.12.8",
    "cross-env": "^6.0.3",
    "nodemon": "^1.19.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "rxjs-compat": "^6.5.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
